music_directory      "/home/kaashif/music"
db_file              "/home/kaashif/.mpd/database"
log_file             "/home/kaashif/.mpd/log"
pid_file             "/home/kaashif/.mpd/pid"
state_file           "/home/kaashif/.mpd/state"
playlist_directory   "/home/kaashif/.mpd/playlists"
bind_to_address      "127.0.0.1"
port                 "6600"
log_level            "verbose"
gapless_mp3_playback "yes"
metadata_to_use      "artist,album,title,track,name,genre,date,composer,performer,disc,albumartist,albumartistsort"
auto_update          "yes"


