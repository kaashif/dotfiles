super + t
	urxvt

super + o
	xset dpms force off

super + c
	firefox

super + b
	emacsclient -c

super + d
	dmenu_normal.sh

super + shift + d
	dmenu_playlist.sh

super + shift + a
	dmenu_add_playlist.sh

XF86Audio{Prev,Next,Play}
	mpc -q {prev,next,toggle}

XF86Audio{Raise,Lower}Volume
	amixer set Master 10%{+,-}

XF86AudioMute
	amixer set Master toggle

